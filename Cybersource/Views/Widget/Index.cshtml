            //var $widget = $(parent.document.getElementById('widget'));
            $(function () {
                $(".close").on('click', function() {
                    //$widget.css('display', 'none');
                });
                $("#submit-button").on('click', function(e) {
                    e.preventDefault();
                    var $el = $(this);
                    if ($el.hasClass('iconContinue')) {
                        $el.removeClass('iconContinue');
                        $el.html('Donate $20');
                        $("#paymentView").css('display', 'block');
                        $("#infoView").css('display', 'none');
                    } else if ($el.html() === 'Close') {
                        //TODO: reset nicely
                        $el.addClass('iconContinue');
                        $el.html('Payment Info');
                        $("#paymentView").css('display', 'none');
                        $("#infoView").css('display', 'block');
                        $("#confirmationView").css('display', 'none');
                        //$widget.css('display', 'none');
                   } else {
                        //submit!
                        //TODO: make this not terrible
                        $("#paymentView").css('display', 'none');
                        $("#confirmationView").css('display', 'block');
                        $el.html('Close');
                    }
                });
            });
        </script>
