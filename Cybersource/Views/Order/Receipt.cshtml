@model dynamic

@{
    ViewBag.Title = "Thanks!";
}

<div class="jumbotron">
    <h1>Thank you for your donation!</h1>
    <p class="lead">You will receive an email confirmation with a receipt shortly</p>
    <p>
        While you are waiting, why not check out a few funds in need of your support
    </p>
    <ul>
        <li><a href="#">The Honey Bee Fund</a></li>
        <li><a href="#">The Climate Change Fund</a></li>
        <li><a href="#">The Tiger Sanctuary Fund</a></li>
    </ul>
</div>

<ul>
    @foreach (var key in Request.Form.AllKeys)
    {
        <li>
            <span>@key -- @Request.Form[key]</span>
        </li>
    }
</ul>